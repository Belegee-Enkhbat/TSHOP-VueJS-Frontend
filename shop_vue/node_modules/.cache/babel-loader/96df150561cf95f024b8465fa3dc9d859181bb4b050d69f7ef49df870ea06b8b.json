{"ast":null,"code":"require(\"@/assets/css/style.min.css\");\nimport axios from 'axios';\nimport ProductBox from '@/components/Productbox.vue';\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      latestProducts: [],\n      latestArticles: []\n    };\n  },\n  components: {\n    ProductBox\n  },\n  mounted() {\n    this.getLatestProducts(), this.getLatestArticles();\n    document.title = 'Home | Glee';\n  },\n  methods: {\n    async getLatestProducts() {\n      this.$store.commit('setIsLoading', true);\n      await axios.get('/api/v1/latest-products/').then(response => {\n        this.latestProducts = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n      this.$store.commit('setIsLoading', false);\n    },\n    async getLatestArticles() {\n      await axios.get('/api/v1/latest-articles/').then(response => {\n        this.latestArticles = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["require","axios","ProductBox","name","data","latestProducts","latestArticles","components","mounted","getLatestProducts","getLatestArticles","document","title","methods","$store","commit","get","then","response","catch","error","console","log"],"sources":["/Users/belgee/Downloads/my_shop-main/shop_vue/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n<main class=\"main\">\n  <section class=\"top-slider\">\n\n<div class=\"top-slider__wrapper\">\n    <div class=\"top-slider__item\">\n        <div class=\"top-slider__content\">\n            <h2 class=\"top-slider__title\">\n                SMART AND\n                TRENDY\n            </h2>\n            <p class=\"top-slider__text\">\n                Dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt\n            </p>\n            <a class=\"top-slider__link\" href=\"#\">\n                Shop Now <span>â†’</span>\n            </a>\n        </div>\n        <img class=\"top-slider__img\" src=\"\" alt=\"slide img\">\n    </div>\n</div>\n</section>\n\n\n<div class=\"about\">\n            <div class=\"about__wrapper\">\n                <div class=\"about__item\">\n                    <img class=\"about__item-img\" src=\"../assets/images/icons/support.svg\" alt=\"Support picture\">\n                    <div class=\"about__item-content1\">\n                        <h5 class=\"about__item-title\">\n                            Support 24/7.\n                        </h5>\n                        <p class=\"about__item-text\">\n                            Contact us 24 hours a day, <br>\n                            7 days a week.\n                        </p>\n                    </div>\n                </div>\n                <div class=\"about__item\">\n                    <img class=\"about__item-img\" src=\"../assets/images/icons/delivery.svg\" alt=\"Delivery icon\">\n                    <div class=\"about__item-content\">\n                        <h5 class=\"about__item-title\">\n                            Delivery.\n                        </h5>\n                        <p class=\"about__item-text\">\n                            Free shipping on all US order.\n                        </p>\n                    </div>\n                </div>\n                <div class=\"about__item\">\n                    <img class=\"about__item-img\" src=\"../assets/images/icons/payment.svg\" alt=\" Credit card icon\">\n                    <div class=\"about__item-content\">\n                        <h5 class=\"about__item-title\">\n                            100% Payment secure.\n                        </h5>\n                        <p class=\"about__item-text\">\n                            We ensure secure payment <br>\n                            with PEV.\n                        </p>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n</main>\n    <div class=\"columns is-multiline\">\n      <div class=\"column is-12\">\n          <h2 class=\"is-size-2 has-text-centered\">Latest products</h2>\n      </div>\n\n      <ProductBox \n        v-for=\"product in latestProducts\"\n        v-bind:key=\"product.id\"\n        v-bind:product=\"product\" />\n\n    </div>\n\n<section class=\"articles\">\n            <div class=\"container\">\n                <div class=\"articles__top\">\n                    <h2 class=\"title\">\n                        Our Insights & Articles\n                    </h2>\n                    <a class=\"articles__link\" href=\"#\">\n                        View All\n                    </a>\n                </div>\n                <div class=\"articles__items\">\n                    <div class=\"articles__item\" \n                    v-for=\"article in latestArticles\"\n                    v-bind:key=\"article.id\">\n                    <router-link class=\"articles__item-imglink\" v-bind:to=\"'/articles' + article.get_absolute_url\">\n                            <img v-bind:src=\"article.get_thumbnail\" class=\"articles__item-image\"  alt=\"article image\">\n                    </router-link>\n                        <a class=\"articles__item-type\">\n                            NEWS\n                        </a>\n                        <div class=\"articles__item-content\">\n                            <router-link v-bind:to=\"'/articles' + article.get_absolute_url\">\n                            <p class=\"articles__item-text\">\n                                {{ article.title }}\n                            </p>\n                            </router-link>\n                            <div class=\"articles__item-info\">\n                                <a class=\"articles__item-date\" href=\"#\">\n                                {{ article.created_at }}\n                                </a>\n                                <a class=\"articles__item-author\" href=\"#\">\n                                 By {{ article.author_name }}\n                                </a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n\n            </div>\n            <div class=\"partners\">\n                <div class=\"container\">\n                    <ul class=\"partners__list\">\n                        <li class=\"partners__list-item\">\n                            <img src=\"../assets/images/partners/1.png\" alt=\"partner logo\">\n                        </li>\n                        <li class=\"partners__list-item\">\n                            <img src=\"../assets/images/partners/2.png\" alt=\"partner logo\">\n                        </li>\n                        <li class=\"partners__list-item\">\n                            <img src=\"../assets/images/partners/3.png\" alt=\"partner logo\">\n                        </li>\n                        <li class=\"partners__list-item\">\n                            <img src=\"../assets/images/partners/4.png\" alt=\"partner logo\">\n                        </li>\n                        <li class=\"partners__list-item\">\n                            <img src=\"../assets/images/partners/5.png\" alt=\"partner logo\">\n                        </li>\n                    </ul>\n                </div>\n            </div>\n        </section>\n  </div>\n</template>\n\n<style lang=\"scss\">\n.top-slider {\n   width: 100%;\n    background-color: #fcd7b6;\n\n    &__content {\n        padding-top: 225px;\n    }\n\n    &__item {\n        display: flex !important;\n        justify-content: space-between;\n    }\n\n    &__title {\n        max-width: 420px;\n        // @extend %rubik-500;\n        font-size: 72px;\n        line-height: 70px;\n        color: #243f4d;\n        margin-bottom: 45px;\n    }\n\n    &__text {\n        // @extend %rubik-400;\n        color: #436372;\n        max-width: 330px;\n        margin-bottom: 45px;\n\n    }\n\n    &__link {\n        background-color: #a3bbc8;\n        padding: 25px 20px 25px 25px;\n        // @extend %rubik-500;\n        color: white;\n        cursor: pointer;\n\n        span {\n            padding-left: 60px;\n            font-size: 20px;\n        }\n    }\n\n    &__img {\n        padding: 100px 180px 0 0;\n    }\n\n\n    .articles {\n    // background-image: url('../images/articles-bg.svg');\n    background-repeat: no-repeat;\n    background-position: 50% 90%;\n\n    &__top {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        margin-bottom: 50px;\n    }\n\n    &__link {\n        // @extend %dmsans-700;\n        font-size: 16px;\n        color: #262626;\n        position: relative;\n        padding-right: 18px;\n\n        &::after {\n            position: absolute;\n            // content: url(\"../images/icons/arrow-right.svg\");\n            width: 6px;\n            height: 11px;\n            right: -5px;\n        }\n    }\n\n    &__items {\n        display: flex;\n        justify-content: space-between;\n        margin-bottom: 90px;\n\n    }\n\n    &__item {\n        box-shadow: 0px 6px 47px 3px rgba(37, 37, 37, 0.05);\n        border-radius: 5px;\n        background-color: #ffffff;\n    }\n\n    &__item-image {\n        max-width: 371px;\n\n        position: relative;\n        margin-bottom: 32px;\n    }\n\n    &__item-type {\n        // @extend %poppins-700;\n        font-size: 14px;\n        color: #ffffff;\n        border: 1px solid rgb(163, 187, 200);\n        padding: 3px 30px;\n        border-radius: 16px;\n        background-color: rgb(163, 187, 200);\n        cursor: pointer;\n        position: absolute;\n        transform: translateY(-49px);\n        margin-left: 28px;\n    }\n\n    &__item-content {\n        padding: 0 28px 28px;\n    }\n\n    &__item-text {\n        // @extend %dmsans-500;\n        font-size: 18px;\n        color: #262626;\n        text-align-last: left;\n        max-width: 315px;\n        border-bottom: 1px solid #e0e0e0;\n        margin-bottom: 18px;\n        height: 84px;\n    }\n\n    &__item-info {\n        display: flex;\n        font-size: 13px;\n        // @extend %dmsans-500;\n\n        a {\n            color: #222933;\n        }\n    }\n\n    &__item-date {\n        margin: 2px 0 30px;\n        margin-left: 2px;\n        margin-right: 30px;\n    }\n\n}\n\n\n}\n</style>\n\n<script>\nrequire(\"@/assets/css/style.min.css\")\nimport axios from 'axios'\n\nimport ProductBox from '@/components/Productbox.vue'\n\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      latestProducts: [],\n      latestArticles: []\n    }\n  },\n  components: {\n    ProductBox\n  },\n  mounted() {\n    this.getLatestProducts(),\n    this.getLatestArticles()\n\n    document.title = 'Home | Glee'\n\n  },\n  methods: {\n    async getLatestProducts() {\n      this.$store.commit('setIsLoading', true)\n\n      await axios\n        .get('/api/v1/latest-products/')\n        .then(response => {\n          this.latestProducts = response.data\n        })\n        .catch(error => {\n          console.log(error)\n        })\n\n      this.$store.commit('setIsLoading', false)\n    },\n\n    async getLatestArticles() {\n        await axios\n            .get('/api/v1/latest-articles/')\n            .then(response => {\n            this.latestArticles = response.data\n            })\n            .catch(error => {\n            console.log(error)\n            })\n    }\n  }\n}\n\n</script>"],"mappings":"AAmSAA,OAAO,CAAC,4BAA4B;AACpC,OAAOC,KAAI,MAAO,OAAM;AAExB,OAAOC,UAAS,MAAO,6BAA4B;AAEnD,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,EAAE;MAClBC,cAAc,EAAE;IAClB;EACF,CAAC;EACDC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,iBAAiB,CAAC,CAAC,EACxB,IAAI,CAACC,iBAAiB,CAAC;IAEvBC,QAAQ,CAACC,KAAI,GAAI,aAAY;EAE/B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMJ,iBAAiBA,CAAA,EAAG;MACxB,IAAI,CAACK,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MAEvC,MAAMd,KAAI,CACPe,GAAG,CAAC,0BAA0B,EAC9BC,IAAI,CAACC,QAAO,I