            <CartItem\n              v-for=\"item in cart.items\"\n              v-bind:key=\"item.product.id\"\n              v-bind:initialItem=\"item\"\n              v-on:removeFromCart=\"removeFromCart\"\n            />\n          </tbody>\n        </table>\n\n        <p v-else>Таны картанд ямар наган бүтээгдэхүүн байхгүй байна...</p>\n      </div>\n\n      <div class=\"column is-12 box\">\n        <h2 class=\"subtitle\">Эцсийн дүн</h2>\n\n        <strong>${{ getTotalPrice.toFixed(2) }}</strong\n        >, {{ getTotalQuantity }} items\n\n        <hr />\n\n        <router-link to=\"/cart/checkout\" class=\"button is-dark\"\n          >Захиалга өгөх</router-link\n        >\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport CartItem from \"@/components/CartItem.vue\";\n\nexport default {\n  name: \"Cart\",\n  components: {\n    CartItem,\n  },\n  data() {\n    return {\n      cart: {\n        items: [],\n      },\n    };\n  },\n  mounted() {\n    this.cart = this.$store.state.cart;\n  },\n  methods: {\n    removeFromCart(item) {\n      this.cart.items = this.cart.items.filter(\n        (i) => i.product.id !== item.product.id\n      );\n    },\n  },\n  computed: {\n    getTotalQuantity() {\n      return this.cart.items.reduce((total, item) => total + item.quantity, 0);\n    },\n    getTotalPrice() {\n      return this.cart.items.reduce((total, item) => total + item.product.price * item.quantity, 0);\n    },\n  },\n};\n</script>\n"],"mappings":";;;AAiDA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,2BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IACVF,QAAQ,EAARA;EACF,CAAC;EACDG,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAO,WAAAA,QAAA,EAAG;IACR,IAAI,CAACF,IAAG,GAAI,IAAI,CAACG,MAAM,CAACC,KAAK,CAACJ,IAAI;EACpC,CAAC;EACDK,OAAO,EAAE;IACPC,cAAc,WAAAA,eAACC,IAAI,EAAE;MACnB,IAAI,CAACP,IAAI,CAACC,KAAI,GAAI,IAAI,CAACD,IAAI,CAACC,KAAK,CAACO,MAAM,CACtC,UAACC,CAAC;QAAA,OAAKA,