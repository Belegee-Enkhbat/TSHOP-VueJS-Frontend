{"ast":null,"code":"import \"core-js/modules/es.array.filter.js\";\nimport \"core-js/modules/es.array.reduce.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport axios from \"axios\";\nimport CartItem from \"@/components/CartItem.vue\";\nexport default {\n  name: \"Cart\",\n  components: {\n    CartItem: CartItem\n  },\n  data: function data() {\n    return {\n      cart: {\n        items: []\n      }\n    };\n  },\n  mounted: function mounted() {\n    this.cart = this.$store.state.cart;\n  },\n  methods: {\n    removeFromCart: function removeFromCart(item) {\n      this.cart.items = this.cart.items.filter(function (i) {\n        return i.product.id !== item.product.id;\n      });\n    }\n  },\n  computed: {\n    cartTotalLength: function cartTotalLength() {\n      return this.cart.items.reduce(function (acc, curVal) {\n        return acc += curVal.quantity;\n      }, 0);\n    },\n    cartTotalPrice: function cartTotalPrice() {\n      return this.cart.items.reduce(function (acc, curVal) {\n        return acc += curVal.product.price * curVal.quantity;\n      }, 0);\n    }\n  }\n};","map":{"version":3,"names":["axios","CartItem","name","components","data","cart","items","mounted","$store","state","methods","removeFromCart","item","filter","i","product","id","computed","cartTotalLength","reduce","acc","curVal","quantity","cartTotalPrice","price"],"sources":["/home/uranbileg/class/TSHOP-VueJS-Frontend/src/views/Cart.vue"],"sourcesContent":["<template>\n  <div class=\"page-cart\">\n    <div class=\"columns is-multiline\">\n      <div class=\"column is-12\">\n        <h1 class=\"title\">Карт</h1>\n      </div>\n\n      <div class=\"column is-12 box\">\n        <table class=\"table is-fullwidth\" v-if=\"cartTotalLength\">\n          <thead>\n            <tr>\n              <th>Бүтээгдэхүүн</th>\n              <th>Үнэ</th>\n              <th>Тоо ширхэг</th>\n              <th>Нийт</th>\n              <th></th>\n            </tr>\n          </thead>\n\n          <tbody>\n            <CartItem\n              v-for=\"item in cart.items\"\n              v-bind:key=\"item.product.id\"\n              v-bind:initialItem=\"item\"\n              v-on:removeFromCart=\"removeFromCart\"\n            />\n          </tbody>\n        </table>\n\n        <p v-else>Таны картанд ямар наган бүтээгдэхүүн байхгүй байна...</p>\n      </div>\n\n      <div class=\"column is-12 box\">\n        <h2 class=\"subtitle\">Эцсийн дүн</h2>\n\n        <strong>${{ cartTotalPrice.toFixed(2) }}</strong\n        >, {{ cartTotalLength }} items\n\n        <hr />\n\n        <router-link to=\"/cart/checkout\" class=\"button is-dark\"\n          >Захиалга өгөх</router-link\n        >\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport CartItem from \"@/components/CartItem.vue\";\n\nexport default {\n  name: \"Cart\",\n  components: {\n    CartItem,\n  },\n  data() {\n    return {\n      cart: {\n        items: [],\n      },\n    };\n  },\n  mounted() {\n    this.cart = this.$store.state.cart;\n  },\n  methods: {\n    removeFromCart(item) {\n      this.cart.items = this.cart.items.filter(\n        (i) => i.product.id !== item.product.id\n      );\n    },\n  },\n  computed: {\n    cartTotalLength() {\n      return this.cart.items.reduce((acc, curVal) => {\n        return (acc += curVal.quantity);\n      }, 0);\n    },\n    cartTotalPrice() {\n      return this.cart.items.reduce((acc, curVal) => {\n        return (acc += curVal.product.price * curVal.quantity);\n      }, 0);\n    },\n  },\n};\n</script>\n"],"mappings":";;;AAiDA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,2BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IACVF,QAAQ,EAARA;EACF,CAAC;EACDG,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAO,WAAAA,QAAA,EAAG;IACR,IAAI,CAACF,IAAG,GAAI,IAAI,CAACG,MAAM,CAACC,KAAK,CAACJ,IAAI;EACpC,CAAC;EACDK,OAAO,EAAE;IACPC,cAAc,WAAAA,eAACC,IAAI,EAAE;MACnB,IAAI,CAACP,IAAI,CAACC,KA